#
# Bio-based asphaltene molecule in an interface of water and a saturated hydrocarbon decane
#



tolerance 2.0
filetype xyz
output interface.xyz

structure water.xyz
  number 300
  inside box -20. 0. 0. 0. 39. 39.
end structure

structure decane.xyz
  number 100
  inside box 0. 0. 0. 21. 39. 39.
end structure

structure asphaltene1.xyz
  centerofmass
  fixed 0. 20. 20. 1.57 1.57 1.57
end structure